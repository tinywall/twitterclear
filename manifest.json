{
  "name": "Twitter Image Shrink",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Black Google.",
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "js": ["jquery-1.7.2.min.js", "onload.js"]
    }
  ],
  "icons": { 
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png" 
	},
  "permissions": [
    "*://twitter.com/*"
  ]
}